<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppBaseTheme">
    </style>
    <style name="AppTheme">
    </style>
    <style name="Board">
        <item name="android:textSize">@dimen/board_text_size</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">clip_horizontal</item>
    </style>
    <style name="Button">
        <item name="android:minWidth">0dp</item>
        <item name="android:minHeight">0dp</item>
    </style>
    <style name="Button.Pink">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">@drawable/btn_pink</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/height_button</item>
        <item name="android:layout_margin">20dp</item>
    </style>
    <style name="Button.White">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/pink</item>
        <item name="android:background">@drawable/btn_white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/height_button</item>
        <item name="android:layout_margin">20dp</item>
    </style>
    <style name="ButtonItemRight">
        <item name="android:background">@drawable/btn_white_flat</item>
        <item name="android:text">@string/off</item>
    </style>
    <style name="Divider">
        <item name="android:background">@color/divider</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>
    <style name="Divider.PaddingLeft">
        <item name="android:layout_marginLeft">15dp</item>
    </style>
    <style name="DividerContent">
        <item name="android:layout_marginLeft">@dimen/margin_10</item>
        <item name="divider_color">@color/gray</item>
        <item name="divider_height">1dp</item>
    </style>
    <style name="FullHeightDialog">
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="FullHeightLight">
        <item name="android:windowNoTitle">true</item>
        <item name="calendarViewStyle">@style/Widget.Holo.Light.CalendarView</item>
        <item name="datePickerStyle">@style/Widget.Holo.Light.DatePicker</item>
        <item name="numberPickerStyle">@style/NPWidget.Holo.Light.NumberPicker</item>
    </style>
    <style name="HeaderText">
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">clip_horizontal</item>
        <item name="android:background">@color/gray_light</item>
        <item name="android:paddingLeft">@dimen/margin_10</item>
    </style>
    <style name="KeyPad">
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:onClick">registerKey</item>
    </style>
    <style name="LabelText">
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
    </style>
    <style name="Layout">
    </style>
    <style name="Layout.FillWidth">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="Layout.FillWidth.Vertical">
        <item name="android:orientation">vertical</item>
    </style>
    <style name="Layout.FillWidthHeight">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="Layout.FillWidthHeight.Vertical">
        <item name="android:orientation">vertical</item>
    </style>
    <style name="Layout.Weight">
        <item name="android:layout_weight">1</item>
    </style>
    <style name="Layout.Weight.Height">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">0dp</item>
    </style>
    <style name="Layout.Weight.HeightFillWidth">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
    </style>
    <style name="Layout.Weight.Width">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="Layout.Weight.WidthFillHeight">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="Layout.WrapWidthHeight">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="Layout.WrapWidthHeight.Vertical">
        <item name="android:orientation">vertical</item>
    </style>
    <style name="LeftTextBlack">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">clip_horizontal</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>
    <style name="LightIfPossible">
    </style>
    <style name="ListView">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:divider">@android:color/transparent</item>
    </style>
    <style name="NPWidget">
        <item name="android:textAppearance">?android:attr/textAppearance</item>
    </style>
    <style name="NPWidget.Holo.Light.NumberPicker">
        <item name="virtualButtonPressedDrawable">@drawable/item_background_holo_light</item>
    </style>
    <style name="NPWidget.Holo.NumberPicker">
        <item name="internalLayout">@layout/number_picker_with_selector_wheel</item>
        <item name="internalMaxHeight">180dp</item>
        <item name="internalMinWidth">64dp</item>
        <item name="selectionDivider">@drawable/np_numberpicker_selection_divider</item>
        <item name="selectionDividerHeight">2dp</item>
        <item name="solidColor">@android:color/transparent</item>
        <item name="virtualButtonPressedDrawable">@drawable/item_background_holo_dark</item>
    </style>
    <style name="NPWidget.NumberPicker">
        <item name="android:orientation">vertical</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:fadingEdgeLength">50dp</item>
    </style>
    <style name="PasscodeInputItemStyle">
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:maxLength">1</item>
        <item name="android:layout_weight">1</item>
        <item name="android:inputType">textLongMessage</item>
    </style>
    <style name="SampleTheme">
        <item name="numberPickerStyle">@style/NPWidget.Holo.NumberPicker</item>
    </style>
    <style name="SampleTheme.Light">
        <item name="android:colorBackground">@color/bg_app</item>
        <item name="android:windowBackground">@color/bg_app</item>
        <item name="android:windowFullscreen">true</item>
        <item name="numberPickerStyle">@style/NPWidget.Holo.Light.NumberPicker</item>
    </style>
    <style name="Sticky">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/text_black</item>
        <item name="android:tag">sticky</item>
        <item name="android:background">#edebf2</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:paddingBottom">5dp</item>
    </style>
    <style name="Text">
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="Text.Content">
        <item name="android:textSize">15sp</item>
    </style>
    <style name="Text.Content.Black">
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="Text.Content.DarkerGray">
        <item name="android:textColor">@android:color/darker_gray</item>
    </style>
    <style name="Text.Content.White">
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="Text.FillWidth">
        <item name="android:layout_width">match_parent</item>
    </style>
    <style name="Text.FillWidth.Bold">
        <item name="android:textStyle">bold</item>
    </style>
    <style name="Text.FillWidth.Italic">
        <item name="android:textStyle">italic</item>
    </style>
    <style name="Text.Header">
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">true</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
    </style>
    <style name="Text.Shadow">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:singleLine">true</item>
        <item name="android:shadowColor">#7f000000</item>
        <item name="android:shadowDx">-1</item>
        <item name="android:shadowDy">-1</item>
        <item name="android:shadowRadius">1</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
    </style>
    <style name="Text.Title">
        <item name="android:textSize">17sp</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">true</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
    </style>
    <style name="Text.Title.Black">
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="Text.Title.Bold">
        <item name="android:textStyle">bold</item>
    </style>
    <style name="Text.Title.DarkerGray">
        <item name="android:textColor">@android:color/darker_gray</item>
    </style>
    <style name="Text.Title.White">
        <item name="android:textColor">@android:color/white</item>
    </style>
